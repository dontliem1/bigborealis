<!DOCTYPE html>
<html lang="en" class="no-js{{ ' home' if page.url === '/' }}">
<!--suppress HtmlRequiredTitleElement -->
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <!--suppress HtmlUnknownTarget -->
  <link rel="icon" href="{{ site.faviconPath }}" type="image/png" />
  {% include "partials/global/meta-info.njk" %}
  <script>document.documentElement.classList.remove('no-js');</script>
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;700&display=swap" rel="stylesheet">
  <link rel='stylesheet' href='{{ '/global.css' | url }}'>
  {% block head %}
  {% endblock %}
</head>
<body class='frame' {% if (background and page.url === '/') %} style='background-image: url("{{ background }}")' {% endif %}>
  <span class='frame__target'></span>
  <span class='frame__swatches'></span>
  <span class='frame__target'></span>
  <span class='frame__left'></span>
  {% block content %}
  {% endblock content %}
  <span class='frame__code'>EJ 01 5790 8702+33</span>
  <span class='frame__scale'>
        <span></span>
    </span>
  <span class='frame__target'></span>
  {% include "partials/global/site-foot.njk" %}
  <span class='frame__target'></span>
  {% block foot %}
  {% endblock %}
</body>
</html>
